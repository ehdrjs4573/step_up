#include <iostream>
using namespace std;

class Tower {
int n;
public:
 Tower() {
   n = 1;
 }
 Tower(int i) {
   n = i;
 }
 int getHeight() {
   return n;
 }
};

int main() {
 Tower myTower;  // 1 λ―Έν„°
 Tower seoulTower(100); // 100 λ―Έν„°
 cout << myTower.getHeight() << endl;
 cout << seoulTower.getHeight() << endl;
}
